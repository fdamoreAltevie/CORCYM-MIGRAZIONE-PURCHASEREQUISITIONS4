{
    "_version": "1.32.0",
    "sap.app": {
        "id": "purchaserequisitionapprovalS4HANA",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZMY_INBOX_RDA_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            },
                "TASKPROCESSING": {
                    "uri": "/sap/opu/odata/IWPGW/TASKPROCESSING;mo;v=2",
                    "type": "OData",
                    "settings": {
                        "odataVersion": "2.0",
                        "localUri": "localService/metadata2.xml"
                    }
                }
        },
        "crossNavigation": {
            "inbounds": {
                "purchaserequisitionapprovalS4HANA-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "rdaapproval",
                    "action": "display",
                    "title": "Purchase Requisition Approval",
                    "subTitle": "",
                    "icon": "sap-icon://inbox",
                    "indicatorDataSource": {
                        "dataSource": "S4HanaCConsoEndpoint_API",
                        "path": "/dynamic_dest/S4HanaCConsoEndpoint_API/sap/opu/odata/IWPGW/TASKPROCESSING;mo;v=2/TaskCollection/$count/?$filter=TaskDefinitionID eq 'TS00008267_WS90000007_0000000361' and (Status eq 'READY' or Status eq 'RESERVED' or Status eq 'IN_PROGRESS' or Status eq 'EXECUTED')",
                        "refresh": 300
                    } 
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "fullWidth": false,
        "icons": {
            "icon": "sap-icon://detail-view",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "purchaserequisitionapprovalS4HANA.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "purchaserequisitionapprovalS4HANA.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true
            },
             "TASKPROCESSING":{
                "dataSource": "TASKPROCESSING",
                "preload": true
            }
        },
        
    "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.f.routing.Router",
                "viewType": "XML",
                "viewPath": "purchaserequisitionapprovalS4HANA.view",
                "controlId": "layout",
                "controlId": "idAppControl",
				"controlAggregation": "detailPages",
                "clearControlAggregation": false,
                "bypassed": {
                    "target": "notFound"
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "master",
                    "target": "master"
                },
                {
                    "pattern": "ExtendedPropertiesRdaSet/{objectId}, {instanceId}",
                    "name": "object",
                    "target": [
                        "master",
                        "object"
                    ]
                }
            ],
              "targets": {
                "master": {
                    "viewName": "Master",
                    "viewLevel": 1,
                    "viewId": "master",
                    "viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"controlAggregation": "masterPages",
					"viewLevel": 0

                }, 
                "object": {
                    "viewName": "Detail",
                    "viewId": "detail",
                    "viewLevel": 2
                },
                "detailObjectNotFound": {
                    "viewName": "DetailObjectNotFound",
                    "viewId": "detailObjectNotFound"
                },
                "notFound": {
                    "viewName": "NotFound",
                    "viewId": "notFound"
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "purchaserequisitionapprovalS4HANA"
    }
}